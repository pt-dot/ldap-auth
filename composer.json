{
    "name": "ptdot/ldapauth",
    "description": "Laravel LDAP authentication wrapper",
    "keywords": ["laravel", "authentication", "ldap"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Didik Tri Susanto",
            "email": "didiktrisusanto@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.3.x|5.4.x|5.5.x|5.6.x"
    },
    "autoload": {
        "psr-4": {
            "Ptdot\\LdapAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ptdot\\LdapAuth\\LdapAuthServiceProvider"
            ],
            "aliases": {
                "LdapAuth": "Ptdot\\LdapAuth\\LdapAuthFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
